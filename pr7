–ó–∞–¥–∞–Ω–∏–µ 1:
print("–°–∏—Å—Ç–µ–º–∞ —Ä–µ–π—Ç–∏–Ω–≥–∞ –ø—Ä–æ–¥—É–∫—Ç–æ–≤")
rating = int(input("–í–≤–µ–¥–∏—Ç–µ —Ä–µ–π—Ç–∏–Ω–≥ –ø—Ä–æ–¥—É–∫—Ç–∞ –æ—Ç 1 –¥–æ 10: "))

match rating:
    case 1| 2| 3:
        print("–ü–ª–æ—Ö–æ–π –ø—Ä–æ–¥—É–∫—Ç!‚ùå –ù–µ —Ä–µ–∫–æ–º–µ–Ω–¥—É—é!")
    case 4| 5| 6:
        print("–°—Ä–µ–¥–Ω–∏–π –ø—Ä–æ–¥—É–∫—Ç ü§î –ú–æ–∂–Ω–æ –ø–æ–ø—Ä–æ–±–æ–≤–∞—Ç—å")
    case 7| 8:
        print("–•–æ—Ä–æ—à–∏–π –ø—Ä–æ–¥—É–∫—Ç!üëç –†–µ–∫–æ–º–µ–Ω–¥—É—é!")
    case 9| 10:
        print("–û—Ç–ª–∏—á–Ω—ã–π –ø—Ä–æ–¥—É–∫—Ç!üåü –ù–∞—Å—Ç–æ—è—Ç–µ–ª—å–Ω–æ —Ä–µ–∫–æ–º–µ–Ω–¥—É—é!")
    case _:
        print("–ù–µ–≤–µ—Ä–Ω–∞—è —Ä–µ–π—Ç–∏–Ω–≥")

–ó–∞–¥–∞–Ω–∏–µ 2:
print("="*20)
print("üì¶ –°–¢–ê–¢–£–° –ó–ê–ö–ê–ó–ê üì¶")
print("="*20)

status = input("–í–≤–µ–¥–∏—Ç–µ —Å—Ç–∞—Ç—É—Å –∑–∞–∫–∞–∑–∞: ")

match status:
    case "pending":
        print("–°—Ç–∞—Ç—É—Å: –≤ –æ–∂–∏–¥–∞–Ω–∏–∏üí≤\n–û–ø–∏—Å–∞–Ω–∏–µ: –û–∂–∏–¥–∞–µ—Ç –æ–ø–ª–∞—Ç—ã\n–ü—Ä–∏–º–µ—Ä–Ω–æ–µ –≤—Ä–µ–º—è –æ–∂–∏–¥–∞–Ω–∏—è: 1 —á–∞—Å\n–†–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–∏: –û–ø–ª–∞—Ç–∏—Ç–µ –∑–∞–∫–∞–∑")
    case "processing":
        print("–°—Ç–∞—Ç—É—Å: –≤ –æ–±—Ä–∞–±–æ—Ç–∫–µ‚åõ\n–û–ø–∏—Å–∞–Ω–∏–µ: –û–∂–∏–¥–∞–µ—Ç –ø–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏—è\n–ü—Ä–∏–º–µ—Ä–Ω–æ–µ –≤—Ä–µ–º—è –æ–∂–∏–¥–∞–Ω–∏—è: 1 —á–∞—Å\n–†–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–∏: –û–∂–∏–¥–∞–π—Ç–µ –ø–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏—è")
    case "shipped":
        print("–°—Ç–∞—Ç—É—Å: –≤ –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω–æ‚úàÔ∏è\n–û–ø–∏—Å–∞–Ω–∏–µ: –í–∞—à –∑–∞–∫–∞–∑ –≤ –ø—É—Ç–∏\n–ü—Ä–∏–º–µ—Ä–Ω–æ–µ –≤—Ä–µ–º—è –æ–∂–∏–¥–∞–Ω–∏—è: 2-5 –¥–Ω–µ–π\n–†–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–∏: –°–ª–µ–¥–∏—Ç–µ –∑–∞ —É–≤–µ–¥–æ–º–ª–µ–Ω–∏—è–º–∏ –æ –¥–æ—Å—Ç–∞–≤–∫–µ")
    case "delivered":
        print("–°—Ç–∞—Ç—É—Å: –¥–æ—Å—Ç–∞–≤–ª–µ–Ω–æüì¶\n–û–ø–∏—Å–∞–Ω–∏–µ: –í–∞—à –∑–∞–∫–∞–∑ –≤ –ø—É–Ω–∫—Ç–µ –≤—ã–¥–∞—á–∏\n–ü—Ä–∏–º–µ—Ä–Ω–æ–µ –≤—Ä–µ–º—è –æ–∂–∏–¥–∞–Ω–∏—è: 14 –¥–Ω–µ–π\n–†–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–∏: –ó–∞–±–µ—Ä–∏—Ç–µ –∑–∞–∫–∞–∑")
    case "cancelled":
        print("–°—Ç–∞—Ç—É—Å: –æ—Ç–º–µ–Ω–µ–Ω–æ‚ùå\n–û–ø–∏—Å–∞–Ω–∏–µ: –í–∞—à –∑–∞–∫–∞–∑ –æ—Ç–º–µ–Ω–µ–Ω")
    case _:
        print("–û—à–∏–±–∫–∞: –ù–µ–∏–∑–≤–µ—Å—Ç–Ω—ã–π —Å—Ç–∞—Ç—É—Å invalid_status\n–î–æ—Å—Ç—É–ø–Ω—ã–µ —Å—Ç–∞—Ç—É—Å—ã: pending (–≤ –æ–∂–∏–¥–∞–Ω–∏–∏), processing (–≤ –æ–±—Ä–∞–±–æ—Ç–∫–µ), shipped (–æ—Ç–ø—Ä–∞–≤–ª–µ–Ω–æ), delivered (–¥–æ—Å—Ç–∞–≤–ª–µ–Ω–æ), cancelled (–æ—Ç–º–µ–Ω–µ–Ω–æ)")
